<?xml version="1.0" encoding="utf-8"?>
<odoo>
<!-- Courses -->
    <record id="course_dragon" model="openacademy.course">
        <field name="name">Dragon 101</field>
        <field name="description">You will learn how to train your dragon</field>
        <field name="responsible_id" ref="datasample.partner_daenerys"/>
        <field name="level">1</field>
    </record>

    <record id="course_zombie" model="openacademy.course">
        <field name="name">Raccoon City - Training Ground</field>
        <field name="description">You will learn how to survive in a deadly city</field>
        <field name="responsible_id" ref="datasample.partner_chris"/>
        <field name="level">2</field>
    </record>
<!-- Sessions -->
    <record id="session_dragon1" model="openacademy.session">
        <field name="name">Dragon 101 - Session 1</field>
        <field name="duration">5</field>
        <field name="seats">6</field>
        <field name="instructor_id" ref="datasample.partner_tyrion"/>
        <field name="course_id" ref="course_dragon"/>
        <field name="state">draft</field>
    </record>

    <record id="session_dragon2" model="openacademy.session">
        <field name="name">Dragon 101 - Session 2</field>
        <field name="duration">5</field>
        <field name="seats">6</field>
        <field name="instructor_id" ref="datasample.partner_daenerys"/>
        <field name="course_id" ref="course_dragon"/>
        <field name="state">draft</field>
    </record>

    <record id="session_zombie1" model="openacademy.session">
        <field name="name">RC Training Ground - Session 1</field>
        <field name="duration">7</field>
        <field name="seats">4</field>
        <field name="instructor_id" ref="datasample.partner_jill"/>
        <field name="course_id" ref="course_zombie"/>
        <field name="state">draft</field>
    </record>
</odoo>